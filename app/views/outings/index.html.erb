<h1>Listing outings</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @outings.each do |outing| %>
  <tr>
    <td><%= outing.name %></td>
    <td><%= outing.address %></td>
    <td><%= outing.price %></td>
    <td><%= link_to 'Show', outing %></td>
    <td><%= link_to 'Edit', edit_outing_path(outing) %></td>
    <td><%= link_to 'Destroy', outing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Outing', new_outing_path %>