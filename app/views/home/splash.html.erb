<h2>The way couples connect with other couples</h2>
<div class="splash">
  <%= form_for("couple", :url => couple_session_path) do |f| %>
    <div>
      <%= f.label :email do %>
        <%= f.text_field :email, :autofocus => true, :placeholder => "Enter your email to get started!" %>
      <% end %>
      <%= f.submit 'Go!', :class => "button" %>
    </div>
  <% end %>  
</div>
