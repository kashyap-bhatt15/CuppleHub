<section id="contact" class="section">
<div class="container">
	<div class="row">
		<div class="span12">
			<div class="heading">
				<h3><span>Get in touch</span></h3>
			</div>
			<div class="sub-heading">
				<p>
					 We are social. We would love to have your feedback.
				</p>
			</div>
		</div>
	</div>
</div>
<div class="map-container">
	<!-- google map code here -->
	<iframe src="http://maps.google.com/maps?f=q&source=s_q&hl=en&geocode=&q=AlleyNYC,7th%20ave&ie=UTF8&z=17&t=m&iwloc=near&output=embed">
	</iframe>
	<!-- end google map code -->
</div>
<div class="container">
	<div class="row">
		<div class="span6">
			<h4><i class="icon-envelope"></i><strong>Contact form</strong></h4>
			<p>
				Want to work with us or just want to say hello? Don't hesitate to get in touch!
			</p>
			<!-- start contact form -->
			<form id="contactform" action="#" method="post" class="contact-form" name="contact-form">
				<ul class="contact-list">
					<li>
					<label>Name <span>*</span></label>
					<input type="text" class="form_input" id="name" name="name" placeholder="Your name" />
					<li>
					<label>Email <span>*</span></label>
					<input type="text" class="form_input" name="email" id="email" placeholder="Your email" />
					</li>
					<li>
					<label>Your message <span>*</span></label>
					<textarea name="message" id="message" rows="12" class="form_textarea" placeholder="Your message"></textarea>
					</li>
					<li class="last">
					<button class="btn btn-large btn-theme" type="submit" id="send">Send a message</button>
					</li>
				</ul>
			</form>
			<!-- end contact form -->
		</div>
		<div class="span6">
			<h4><i class="icon-suitcase"></i><strong>Our office</strong></h4>
			<p>
				 Feel free to ask if you have any question regarding our platform or if you just want to say Hello, we would love to hear from you.
			</p>
			<div class="dotted_line">
			</div>
			<p>
				<strong>CuppleHub at Startup Weekend</strong>
			</p>
			<p>
				Floor 17A, 500 7th Avenue, New York, NY 10018
			</p>
			<p>
				<span><i class="icon-phone"></i><strong>Phone:</strong> (212) 960-8543</span>
			</p>
			<p>
				<span><i class="icon-comment"></i><strong>Skype:</strong> CuppleHub</span>
			</p>
			<p>
				<span><i class="icon-envelope-alt"></i><strong>Email:</strong> info@cupplehub.com</span>
			</p>
			<div class="dotted_line">
			</div>
			<h4><i class="icon-group"></i><strong>Connect to our social profiles</strong></h4>
			<div class="social-links">
				<ul class="social-links">
					<li><a href="#" title="Twitter"><i class="icon-square icon-32 icon-twitter"></i></a></li>
					<li><a href="#" title="Facebook"><i class="icon-square icon-32 icon-facebook"></i></a></li>
